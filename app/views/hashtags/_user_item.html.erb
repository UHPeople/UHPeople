<li class="search-result result-user member-<%= user.id %>" style="min-height: 59px; padding-left:8px;">
  <div class="result-image" style="padding-top: 4px !important;">
    <%= link_to image_tag(user.profile_picture_url(:small), class: "img-circle"), user %>
  </div>
  <div class="result-title">
    <p><%= link_to user.name, user %></p>
  </div>
</li>
