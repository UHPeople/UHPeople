<%= form_tag({controller: "photos", action: "create"}, method: "post", multipart: true) do %>
        <div class="field">
      <%= label_tag :image_text, "Photo title" %><br>
      <%= text_field_tag :image_text %>
    </div>
    <div class="field">
      <%= label_tag :image %><br>
      <%= file_field_tag :image %>
    </div>
    <div class="actions">
      <%= submit_tag("Add photo", :class=>"btn btn-primary") %>
    </div>
<% end %>
