<p id="notice"><%= notice %></p>

<table class="table table-condensed table-hover">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Login as', "login/#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>